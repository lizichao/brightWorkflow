<script type="text/javascript">
//设置分页信息
	function setPageInfo(data){
		var customPageInfoObject = data.Data2 && data.Data2[0];
	    var pageInfoObject = customPageInfoObject || data.PageInfo;
		var pageNo = parseInt(pageInfoObject.PageNo);
		var pageCount = parseInt(pageInfoObject.PageCount);
		var startstr;
	    var endstr;
		if(pageNo==1){
		  $("#ext-comp-1013").addClass("x-item-disabled");
		  $("#ext-comp-1014").addClass("x-item-disabled");
		  $("#ext-gen68").attr("disabled", "disabled");
		  $("#ext-gen70").attr("disabled", "disabled");
		  startstr=1;
		}else{
		  $("#ext-comp-1013").removeClass("x-item-disabled");
		  $("#ext-comp-1014").removeClass("x-item-disabled");
		  $("#ext-gen68").attr("disabled", false);
		  $("#ext-gen70").attr("disabled", false);
		  startstr=(pageNo-1)*pageInfoObject.PageSize+1
		}
		if(pageNo==pageCount){
		  $("#ext-comp-1017").addClass("x-item-disabled");
		  $("#ext-comp-1018").addClass("x-item-disabled");
		  $("#ext-gen79").attr("disabled", "disabled");
		  $("#ext-gen81").attr("disabled", "disabled");
		  endstr=pageInfoObject.RecordCount;
		}else{
		  $("#ext-comp-1017").removeClass("x-item-disabled");
		  $("#ext-comp-1018").removeClass("x-item-disabled");
		  $("#ext-gen79").attr("disabled", false);
		  $("#ext-gen81").attr("disabled", false);
		  endstr=pageNo*pageInfoObject.PageSize
		}
		$("#pageNo").val(pageNo);
		$("#pageCount").val(pageCount);
		$("#ext-comp-1016").html("页，共"+pageCount+"页");
		$("#recordCount").html("显示"+startstr+"--"+endstr+"条，共"+pageInfoObject.RecordCount+" 条");
	}
	function setMethod(methodName,parameter){
	   $("#ext-gen68").attr("onclick",methodName+"("+parameter+",1)");      //第一页
	   $("#ext-gen70").attr("onclick",methodName+"("+parameter+",parseInt($('#pageNo').val())-1)");      //上一页
	   $("#pageNo").attr("onkeydown","if(event.keyCode==13){ if(parseInt(this.value)<=parseInt($('#pageCount').val())){"+methodName+"("+parameter+",this.value)}else{"+methodName+"("+parameter+",$('#pageCount').val())}}");         //文本框
	   $("#ext-gen79").attr("onclick",methodName+"("+parameter+",parseInt($('#pageNo').val())+1)");          //下一页
	   $("#ext-gen81").attr("onclick",methodName+"("+parameter+",$('#pageCount').val())");          //最后一页
	}
</script>