    classnm : "Ext.form.BasicForm",
    desc : "<b>Boolean</b> Set to true if this form is a file upload."
  },{
    name : "formId",
    desc : "<b>String</b> The id of the FORM tag (defaults to an auto-generated id)"
  },{
